<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".view.VMap"
    android:id="@+id/layoutDrawer">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorWhiteForFFFFFF">
        <FrameLayout
            android:id="@+id/layoutMap"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/imageCheckInInMap"
            android:layout_below="@+id/viewBarTimerAInMap"
            android:layout_marginBottom="8dp"/>
        <View
            android:id="@+id/viewBarOneInMap"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="12dp"
            android:background="@color/colorWhiteForFFFFFF"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"/>
        <ImageView
            android:id="@+id/imageMenuInMap"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:padding="12dp"
            app:srcCompat="@drawable/btn_list_v1_2"
            android:layout_marginTop="12dp"
            android:layout_marginStart="8dp"
            android:contentDescription="@string/content_description"/>
        <TextView
            android:id="@+id/textBackInMap"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:text="@string/global_back"
            android:gravity="center"
            android:textSize="20sp"
            android:layout_marginTop="12dp"
            android:layout_marginStart="8dp"
            android:visibility="gone"/>
        <EditText
            android:id="@+id/editSearchInMap"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginEnd="66dp"
            android:layout_marginStart="56dp"
            android:layout_marginTop="12dp"
            android:hint="@string/map_search_hint"
            android:paddingStart="8dp"
            android:paddingEnd="8dp"
            android:textSize="16sp"
            android:imeOptions="actionSearch"
            android:maxLines="1"
            android:inputType="text"
            android:background="@color/colorGrayForA0FFFFFF"/>
        <TextView
            android:id="@+id/textTypeTitleInMap"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginEnd="80dp"
            android:layout_marginStart="56dp"
            android:layout_marginTop="12dp"
            android:gravity="center_vertical"
            android:textSize="16sp"
            android:paddingStart="8dp"
            android:paddingEnd="8dp"
            android:visibility="gone"/>
        <ImageView
            android:id="@+id/imageCheckInMap"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="18dp"
            android:layout_marginTop="12dp"
            android:padding="6dp"
            android:contentDescription="@string/content_description"
            android:src="@drawable/notification_v1_1" />
        <ImageView
            android:id="@+id/imageAddDelInMap"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="12dp"
            android:contentDescription="@string/content_description"
            android:padding="12dp"
            android:visibility="gone" />
        <ImageView
            android:id="@+id/imageListInMap"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/btn_list_v1_2"
            android:layout_toStartOf="@+id/imageAddDelInMap"
            android:layout_marginTop="12dp"
            android:padding="12dp"
            android:contentDescription="@string/content_description"
            android:visibility="gone"/>
        <View
            android:id="@+id/viewBarInMap"
            android:layout_width="1dp"
            android:layout_height="1dp"
            android:layout_centerHorizontal="true"/>
        <!--快捷鍵元件區-->
        <TextView
            android:id="@+id/textCheckInInMap"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_alignParentBottom="true"
            android:layout_toStartOf="@+id/textSearchInMap"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="32dp"
            android:maxLines="1"
            android:ellipsize="end"
            android:textSize="12sp"
            android:gravity="center" />
        <ImageView
            android:id="@+id/imageCheckInInMap"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_above="@+id/textCheckInInMap"
            android:layout_alignStart="@+id/textCheckInInMap"
            android:contentDescription="@string/content_description"
            android:src="@drawable/m_1_v11"/>
        <TextView
            android:id="@+id/textSearchInMap"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_alignParentBottom="true"
            android:layout_toStartOf="@+id/viewBarInMap"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="16dp"
            android:maxLines="1"
            android:ellipsize="end"
            android:textSize="12sp"
            android:gravity="center"/>
        <ImageView
            android:id="@+id/imageSearchInMap"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_above="@+id/textSearchInMap"
            android:layout_alignStart="@+id/textSearchInMap"
            android:contentDescription="@string/content_description"
            android:src="@drawable/m_2_v11"/>
        <TextView
            android:id="@+id/textTimerInMap"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/viewBarInMap"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="8dp"
            android:ellipsize="end"
            android:maxLines="1"
            android:textSize="12sp"
            android:gravity="center"/>
        <ImageView
            android:id="@+id/imageTimerInMap"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_above="@+id/textTimerInMap"
            android:layout_alignStart="@+id/textTimerInMap"
            android:contentDescription="@string/content_description"
            android:src="@drawable/m_3_v11"/>
        <TextView
            android:id="@+id/textStringInMap"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/textTimerInMap"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="32dp"
            android:maxLines="1"
            android:ellipsize="end"
            android:textSize="12sp"
            android:gravity="center"/>
        <ImageView
            android:id="@+id/imageStringInMap"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_above="@+id/textStringInMap"
            android:layout_alignStart="@+id/textStringInMap"
            android:contentDescription="@string/content_description"
            android:src="@drawable/m_4_v11" />
        <!--快捷鍵元件區末端-->
        <ImageView
            android:id="@+id/imageHotKeyInMap"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignBottom="@+id/layoutMap"
            android:layout_alignEnd="@+id/layoutMap"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="30dp"
            android:src="@drawable/locatemore_v1_2"
            android:contentDescription="@string/content_description"
            android:background="@drawable/bg_white_circle_full"
            android:elevation="10dp"/>
        <ImageView
            android:id="@+id/imageFocusInMap"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignBottom="@+id/layoutMap"
            android:layout_alignEnd="@+id/layoutMap"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="100dp"
            android:src="@drawable/locateme_v1_2"
            android:contentDescription="@string/content_description"
            android:background="@drawable/bg_white_circle_full"
            android:elevation="10dp" />
        <!--遛狗計時元件區預設隱藏-->
        <View
            android:id="@+id/viewBarTimerAInMap"
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:background="@color/colorWhiteForFFFFFF"
            android:clickable="true"
            android:focusable="true"
            android:visibility="gone"/>
        <View
            android:id="@+id/viewBarTimerBInMap"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/viewBarTimerAInMap"
            android:clickable="true"
            android:focusable="true"
            android:visibility="gone"/>
        <ImageView
            android:id="@+id/imageMarkInMap"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignStart="@+id/viewBarTimerAInMap"
            android:layout_alignTop="@+id/viewBarTimerAInMap"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:contentDescription="@string/content_description"
            android:src="@drawable/placeholder"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textGpsStateInMap"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:contentDescription="@string/content_description"
            android:layout_toStartOf="@+id/viewBarInMap"
            android:layout_marginEnd="4dp"
            android:layout_alignTop="@+id/viewBarTimerAInMap"
            android:layout_marginTop="8dp"
            android:background="@color/colorBlueFor6398D9"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textTitleGpsInMap"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_toEndOf="@+id/viewBarInMap"
            android:layout_alignTop="@+id/viewBarTimerAInMap"
            android:layout_marginTop="8dp"
            android:text="GPS"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textCenterInMap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/imageMarkInMap"
            android:gravity="center"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textCenterSubInMap"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/textCenterInMap"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:maxLines="1"
            android:text="TOTAL TIME"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textLeftSubInMap"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_alignBottom="@+id/viewBarTimerAInMap"
            android:layout_marginStart="50dp"
            android:layout_marginBottom="16dp"
            android:gravity="center"
            android:text="DISTANCE"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textLeftInMap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textLeftSubInMap"
            android:layout_marginBottom="8dp"
            android:layout_alignStart="@+id/textLeftSubInMap"
            android:layout_alignEnd="@+id/textLeftSubInMap"
            android:text="0.00"
            android:gravity="center"
            android:maxLines="1"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textRightSubInMap"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_alignBottom="@+id/viewBarTimerAInMap"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="50dp"
            android:layout_marginBottom="16dp"
            android:text="AVG. PACE"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textRightInMap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textRightSubInMap"
            android:layout_marginBottom="8dp"
            android:gravity="center"
            android:maxLines="1"
            android:layout_alignStart="@+id/textRightSubInMap"
            android:layout_alignEnd="@+id/textRightSubInMap"
            android:visibility="gone"/>
        <ImageView
            android:id="@+id/imageCameraInMap"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="16dp"
            android:layout_marginStart="16dp"
            android:contentDescription="@string/content_description"
            android:src="@drawable/camera_3_v1_1"
            android:padding="8dp"
            android:visibility="gone"/>
        <ImageView
            android:id="@+id/imageLockInMap"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_marginBottom="16dp"
            android:layout_marginEnd="16dp"
            android:contentDescription="@string/content_description"
            android:src="@drawable/pr_lock_v1_1"
            android:padding="8dp"
            android:visibility="gone"/>
        <ImageView
            android:id="@+id/imagePauseInMap"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="16dp"
            android:contentDescription="@string/content_description"
            android:src="@android:drawable/ic_media_pause"
            android:visibility="gone" />
        <ImageView
            android:id="@+id/imageStopInMap"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="16dp"
            android:layout_marginEnd="4dp"
            android:layout_toStartOf="@+id/viewBarInMap"
            android:contentDescription="@string/content_description"
            android:src="@android:drawable/checkbox_off_background"
            android:visibility="gone" />
        <ImageView
            android:id="@+id/imageStartInMap"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="16dp"
            android:layout_marginStart="4dp"
            android:layout_toEndOf="@+id/viewBarInMap"
            android:contentDescription="@string/content_description"
            android:src="@android:drawable/ic_media_play"
            android:visibility="gone"/>
        <ImageView
            android:id="@+id/imageLockBigInMap"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="16dp"
            android:contentDescription="@string/content_description"
            android:src="@android:drawable/ic_lock_lock"
            android:visibility="gone"/>
        <!--遛狗計時元件區末端-->
        <!--EBike元件區預設隱藏-->
        <TextView
            android:id="@+id/textMessageInMap"
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="80dp"
            android:background="@drawable/bg_white_message_map_full"
            android:gravity="center"
            android:textSize="16sp"
            android:elevation="10dp"
            android:visibility="gone"/>
        <View
            android:id="@+id/viewBarBInMap"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:background="@color/colorWhiteForFFFFFF"
            android:layout_alignStart="@+id/textDurationInMap"
            android:layout_alignTop="@+id/textDurationInMap"
            android:layout_alignEnd="@+id/textDurationInMap"
            android:elevation="10dp"
            android:visibility="gone"/>
        <View
            android:id="@+id/viewBarCInMap"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:background="@color/colorWhiteForFFFFFF"
            android:layout_alignStart="@+id/textDurationInMap"
            android:layout_alignTop="@+id/textDurationInMap"
            android:layout_alignEnd="@+id/textCostInMap"
            android:elevation="10dp"
            android:visibility="gone"/>
        <ImageView
            android:id="@+id/imageBillingInMap"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_below="@+id/viewBarOneInMap"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:contentDescription="@string/content_description"
            android:src="@drawable/clock_v1_1"
            android:background="@color/colorWhiteForFFFFFF"
            android:elevation="10dp"
            android:visibility="gone"
            android:padding="8dp"/>
        <TextView
            android:id="@+id/textDurationInMap"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_alignTop="@+id/imageBillingInMap"
            android:gravity="center_vertical"
            android:layout_alignStart="@+id/textCostInMap"
            android:paddingStart="8dp"
            android:paddingEnd="50dp"
            android:elevation="10dp"
            android:textSize="20sp"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textCostInMap"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_below="@+id/textDurationInMap"
            android:layout_toEndOf="@+id/imageBillingInMap"
            android:gravity="center_vertical"
            android:paddingStart="8dp"
            android:paddingEnd="50dp"
            android:elevation="10dp"
            android:textSize="20sp"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textPauseInMap"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:gravity="center"
            android:text="暫停"
            android:layout_alignTop="@+id/textStartInMap"
            android:layout_toStartOf="@+id/textStartInMap"
            android:background="@drawable/bg_white_message_map_full"
            android:elevation="10dp"
            android:layout_marginEnd="20dp"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textStartInMap"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:gravity="center"
            android:text="開始"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:background="@drawable/bg_white_message_map_full"
            android:layout_marginBottom="80dp"
            android:elevation="10dp"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textStopInMap"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:background="@drawable/bg_white_message_map_full"
            android:layout_alignTop="@+id/textStartInMap"
            android:layout_toEndOf="@+id/textStartInMap"
            android:elevation="10dp"
            android:gravity="center"
            android:text="結束"
            android:layout_marginStart="20dp"
            android:visibility="gone"/>
        <!--EBike元件區末端-->
        <!--地標列表元件區預設隱藏-->
        <TextView
            android:id="@+id/textBackLeftInMap"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="12dp"
            android:layout_marginStart="8dp"
            android:text="@string/global_back"
            android:gravity="center"
            android:textStyle="bold"
            android:visibility="gone"/>
        <ImageView
            android:id="@+id/imageBackRightInMap"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="16dp"
            android:contentDescription="@string/content_description"
            android:src="@drawable/list_s_v12"
            android:visibility="gone"
            android:padding="10dp"/>
        <TextView
            android:id="@+id/textBackBottomInMap"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentBottom="true"
            android:background="@color/colorWhiteForFFFFFF"
            android:text="@string/map_show_list"
            android:textColor="@color/colorBlueFor6398D9"
            android:textStyle="bold"
            android:gravity="center_vertical"
            android:textSize="20sp"
            android:paddingStart="16dp"
            android:paddingEnd="16dp"
            android:visibility="gone"/>
        <!--地標列表元件區末端-->
        <!--地標資訊元件區預設隱藏-->
        <View
            android:id="@+id/viewBarAInMap"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:background="@drawable/bg_white_full"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="40dp"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="16dp"
            android:elevation="10dp"
            android:visibility="gone"
            android:clickable="true"
            android:focusable="true"/>
        <TextView
            android:id="@+id/textTitleForInfoInMap"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_alignTop="@+id/viewBarAInMap"
            android:layout_alignStart="@+id/viewBarAInMap"
            android:layout_toStartOf="@+id/imageCollectInMap"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="8dp"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:textStyle="bold"
            android:textSize="16sp"
            android:elevation="10dp"
            android:ellipsize="end"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textRatingInMap"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_below="@+id/textTitleForInfoInMap"
            android:layout_alignStart="@+id/textTitleForInfoInMap"
            android:layout_marginTop="4dp"
            android:gravity="center"
            android:visibility="gone"
            android:elevation="10dp"/>
        <RatingBar
            android:id="@+id/ratingInMap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:theme="@style/RatingBar"
            style="@style/Widget.AppCompat.RatingBar.Small"
            android:layout_alignTop="@+id/textRatingInMap"
            android:layout_toEndOf="@+id/textRatingInMap"
            android:visibility="gone"
            android:elevation="10dp"/>
        <TextView
            android:id="@+id/textCountInMap"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_alignTop="@+id/textRatingInMap"
            android:layout_toEndOf="@+id/ratingInMap"
            android:gravity="center"
            android:visibility="gone"
            android:elevation="10dp"/>
        <TextView
            android:id="@+id/textTypeInMap"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_below="@+id/textRatingInMap"
            android:layout_marginTop="4dp"
            android:layout_alignStart="@+id/textTitleForInfoInMap"
            android:layout_alignEnd="@+id/viewBarAInMap"
            android:layout_marginEnd="16dp"
            android:elevation="10dp"
            android:gravity="center_vertical"
            android:visibility="gone"/>
        <TextView
            android:id="@+id/textStatusInMap"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_below="@+id/textTypeInMap"
            android:layout_marginTop="4dp"
            android:layout_alignStart="@+id/textTitleForInfoInMap"
            android:gravity="center_vertical"
            android:textColor="@color/colorRedForFF2D2D"
            android:visibility="gone"
            android:elevation="10dp"/>
        <TextView
            android:id="@+id/textTimeInMap"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_alignTop="@+id/textStatusInMap"
            android:layout_toEndOf="@+id/textStatusInMap"
            android:layout_alignEnd="@+id/viewBarAInMap"
            android:layout_marginEnd="8dp"
            android:gravity="center_vertical"
            android:visibility="gone"
            android:elevation="10dp"/>
        <ImageView
            android:id="@+id/imageSaveInMap"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignEnd="@+id/viewBarAInMap"
            android:layout_alignTop="@+id/viewBarAInMap"
            android:layout_marginEnd="16dp"
            android:layout_marginTop="16dp"
            android:contentDescription="@string/content_description"
            android:src="@drawable/mark_off_v12"
            android:padding="4dp"
            android:background="@drawable/bg_white_circle_full"
            android:visibility="gone"
            android:elevation="10dp"/>
        <ImageView
            android:id="@+id/imageCollectInMap"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignTop="@+id/imageSaveInMap"
            android:layout_toStartOf="@+id/imageSaveInMap"
            android:layout_marginEnd="8dp"
            android:src="@drawable/add_v1_1"
            android:padding="6dp"
            android:elevation="10dp"
            android:background="@drawable/bg_white_circle_full"
            android:contentDescription="@string/content_description"
            android:visibility="gone"/>

        <include layout="@layout/layout_item_four_ebike"/>
        <include layout="@layout/layout_item_five_ebike"/>
        <include layout="@layout/layout_item_nine_ebike"/>
        <include layout="@layout/layout_item_three_ebike"/>
        <include layout="@layout/layout_item_three_finish_ebike"/>
        <RelativeLayout
            android:id="@+id/layoutMobileMusic"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorBlackFor66000000"
            android:elevation="20dp"
            android:visibility="gone">
            <View
                android:id="@+id/viewBarInMapMobileMusic"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_alignParentBottom="true"
                android:background="@drawable/bg_white_full"
                android:layout_marginStart="32dp"
                android:layout_marginEnd="32dp"
                android:layout_marginBottom="16dp"/>
            <TextView
                android:id="@+id/textCancelInMapMobileMusic"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:layout_alignTop="@+id/viewBarInMapMobileMusic"
                android:layout_alignStart="@+id/viewBarInMapMobileMusic"
                android:layout_marginTop="25dp"
                android:layout_marginStart="32dp"
                android:background="@color/colorBlackFor000000"
                android:textColor="@color/colorWhiteForFFFFFF"
                android:text="@string/map_mobile_back"
                android:gravity="center"
                android:textSize="16sp"/>
            <TextView
                android:id="@+id/textNextInMapMobileMusic"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:layout_alignTop="@+id/viewBarInMapMobileMusic"
                android:layout_alignEnd="@+id/viewBarInMapMobileMusic"
                android:layout_marginTop="25dp"
                android:layout_marginEnd="32dp"
                android:background="@color/colorBlackFor000000"
                android:textColor="@color/colorWhiteForFFFFFF"
                android:text="@string/map_mobile_play"
                android:gravity="center"
                android:textSize="16sp"/>
            <ImageView
                android:id="@+id/imagePictureInMapMobileMusic"
                android:layout_width="332dp"
                android:layout_height="320dp"
                android:scaleType="fitXY"
                android:contentDescription="@string/content_description"
                android:layout_centerHorizontal="true"
                android:layout_above="@+id/viewBarInMapMobileMusic"
                android:layout_marginBottom="32dp"/>
            <ImageView
                android:id="@+id/imageLikeInMapMobileMusic"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignEnd="@+id/imagePictureInMapMobileMusic"
                android:layout_alignTop="@+id/imagePictureInMapMobileMusic"
                android:layout_marginEnd="8dp"
                android:layout_marginTop="8dp"
                android:contentDescription="@string/content_description"/>
            <TextView
                android:id="@+id/textTitleInMapMobileMusic"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_alignBottom="@+id/imagePictureInMapMobileMusic"
                android:layout_alignStart="@+id/imagePictureInMapMobileMusic"
                android:layout_alignEnd="@+id/imagePictureInMapMobileMusic"
                android:layout_centerHorizontal="true"
                android:background="@color/colorBlackFor66000000"
                android:ellipsize="end"
                android:gravity="center"
                android:maxLines="1"
                android:textColor="@color/colorWhiteForFFFFFF" />
            <TextView
                android:id="@+id/textCountsInMapMobileMusic"
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:layout_alignBottom="@+id/imagePictureInMapMobileMusic"
                android:layout_alignEnd="@+id/imagePictureInMapMobileMusic"
                android:layout_marginBottom="68dp"
                android:layout_marginEnd="8dp"
                android:background="@color/colorBlackFor66000000"
                android:ellipsize="end"
                android:gravity="center"
                android:maxLines="1"
                android:textColor="@color/colorWhiteForFFFFFF" />
        </RelativeLayout>
    </RelativeLayout>
    <ListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="@color/colorWhiteForFFFFFF"
        android:clipToPadding="false"
        android:divider="@null"
        android:scrollbars="none"/>
</androidx.drawerlayout.widget.DrawerLayout>